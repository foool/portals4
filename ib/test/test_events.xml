<?xml version="1.0"?>
<test>
  <repeat count="2">
  <subtest>
    <desc>Test events with put to me</desc>
    <ptl>
      <ptl_ni>
	<ptl_eq>
	  <ptl_pt pt_index="2">
	    <ptl_me me_opt="OP_PUT" user_ptr="654" uid="ANY" pt_index="2" me_match="0xa5a5" me_ignore="0xff00">
	      <ptl_eq>
		<ptl_md>
		  <ptl_put ack_req="ACK" user_ptr="432" match="0x5aa5" pt_index="2"/>
		  <msleep count="10"/>
		  <ptl_eq_poll eq_handle="eq[0]">
		    <check which="INVALID"/>
		    <check event_type="PUT"/>
		    <check event_match="0x5aa5"/>
		    <check event_user_ptr="654"/>
		    <check event_ni_fail="OK"/>
		  </ptl_eq_poll>
		  <ptl_eq_poll eq_handle="eq[0]" ret="EQ_EMPTY"/>
		  <ptl_eq_poll eq_handle="eq[1]">
		    <check which="INVALID"/>
		    <check event_type="SEND"/>
		    <check event_user_ptr="432"/>
		    <check event_ni_fail="OK"/>
		  </ptl_eq_poll>
		  <ptl_eq_poll eq_handle="eq[1]">
		    <check which="INVALID"/>
		    <check event_type="ACK"/>
		    <check event_user_ptr="432"/>
		    <check event_ni_fail="OK"/>
		  </ptl_eq_poll>
		  <ptl_eq_poll eq_handle="eq[1]" ret="EQ_EMPTY"/>
	      </ptl_md>
	      </ptl_eq>
	    </ptl_me>
	  </ptl_pt>
	</ptl_eq>
      </ptl_ni>
    </ptl>
  </subtest>
