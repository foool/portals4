<?xml version="1.0"?>
<test>
  <subtest>
    <desc>Test PtlPut with good parameters to ME</desc>
    <ptl>
      <ptl_ni desired_max_msg_size="4096">
	<ptl_pt>
	  <ptl_me me_opt="OP_PUT" me_length="4096" type="UINT" me_data="0x00000000">
            <ptl_md md_length="4096" type="UINT" md_data="0xa5a5a5a5">
              <ptl_put length="4096"/>
	      <msleep count="10"/>
            </ptl_md>
	    <check length="4096" type="UINT" me_data="0xa5a5a5a5"/>
	  </ptl_me>
	</ptl_pt>
      </ptl_ni>
    </ptl>
  </subtest>
