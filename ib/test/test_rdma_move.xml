              <ptl_get length="4096"/>
	      <msleep count="10"/>
	      <check length="4096" type="UINT" md_data="0x5a5a5a5a"/>
            </ptl_md>
	  </ptl_me>
	</ptl_pt>
      </ptl_ni>
    </ptl>
  </subtest>
